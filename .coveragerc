# .coveragerc
[run]
source = TypedUnit
branch = True
relative_files = True
parallel = True
omit =
    **/__init__.py
    TypedUnit/_version.py
    TypedUnit/directories.py
    TypedUnit/**/__main__.py

[report]
skip_covered = True
show_missing = True
precision = 2

[html]
directory = htmlcov

[xml]
output = coverage.xml
